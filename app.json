{
  "expo": {
    "name": "Cerebrus",
    "slug": "Cerebrus",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/xl-space-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.creaitors.cerebrus"
    },
    "android": {
      "package": "com.creaitors.cece",
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/space-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "enableProguardInReleaseBuilds": true
          }
        }
      ]
    ]
  }
}
